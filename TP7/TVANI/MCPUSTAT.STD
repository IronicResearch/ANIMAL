(* MCPU status decoding routines. *)(* MCPU = DM STD motion control system. *)(* DECODE STATUS translates an MCPU axis status byte into a message string. *)(* MCPU command set for DM STD system. *)FUNCTION DECODE_STATUS (VAR M : MOTOR_TABLE) : STRING;BEGIN   CASE M.STAT OF        'D' : DECODE_STATUS := 'Drive Done';        'M' : DECODE_STATUS := 'Motor Moving';        'S' : DECODE_STATUS := 'Switch Stop';        'E' : DECODE_STATUS := 'Existance Error';        ELSE  DECODE_STATUS := '???';        END;END;(* DECODE QUICK translates an MCPU axis status byte into a quick message. *)(* MCPU command set for DM STD system. *)FUNCTION DECODE_QUICK (VAR M : MOTOR_TABLE) : STRING;BEGIN   CASE M.STAT OF        'D' : DECODE_QUICK := 'Done';        'M' : DECODE_QUICK := 'Moving';        'S' : DECODE_QUICK := 'Switch';        'E' : DECODE_QUICK := 'Error';        ELSE  DECODE_QUICK := '???';        END;END;(* DECODE MOTION decodes an MCPU axis status byte for motion. *)(* MCPU command set for DM STD system. *)FUNCTION DECODE_MOTION (VAR M : MOTOR_TABLE) : BOOLEAN;BEGIN   DECODE_MOTION := (M.STAT = 'M');END;